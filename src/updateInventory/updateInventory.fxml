<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="updateInventory.updateInventoryController">
   <children>
      <TableView fx:id="tableView" layoutX="4.0" layoutY="157.0" prefHeight="229.0" prefWidth="593.0">
         <columns>
            <TableColumn fx:id="codeCol" prefWidth="98.0" text="Item Code" />
            <TableColumn fx:id="nameCol" minWidth="0.0" prefWidth="156.0" text="Item Name" />
            <TableColumn fx:id="desCol" minWidth="8.0" prefWidth="203.0" text="Item Description" />
            <TableColumn fx:id="qtyCol" prefWidth="135.0" text="Item Quantity" />
         </columns>
      </TableView>
      <TextField fx:id="textItemcode" layoutX="67.0" layoutY="64.0" prefHeight="25.0" prefWidth="143.0" promptText="Item Code" />
      <TextField fx:id="textItemQty" layoutX="250.0" layoutY="64.0" promptText="Item Quantity" />
      <Button fx:id="addItemButton" layoutX="468.0" layoutY="64.0" mnemonicParsing="false" onAction="#addItem" text="Add Items" />
   </children>
</AnchorPane>
